{% case settings.heading_font_type %}
{% when 'font_regular' %}
    {% assign heading_font = settings.heading_font_regular_family %}
{% when 'font_custom' %}
    {% assign heading_font = settings.heading_font_custom_family %}
{% endcase %}

.column_center h1,
.column_center h2,
.column_center h3,
.column_center h4,
.column_center h5,
.column_center h6 {
	padding: 0 0 4px 0;
	font: {{ settings.heading_font_size }}/1em {{ heading_font }};
	color: {{ settings.heading_font_color }};
	text-transform: uppercase;
  	text-align: center;
}

.column_center h2,
.column_center h3,
.column_center h4 {
	font-size: 15px;
}

.column_center h5,
.column_center h6 {
	font-size: 13px;
}

.column_center .page_heading {
	margin: 0;
	padding: 38px 0 5px 0;
  	line-height: 44px;
}
.column_center .page_heading a {
	float: right;
}



/* 4.2 PARAGRAPHS & LISTS */
p {
	margin: 15px 0;
	padding: 0;
}

.column_center ul {
	list-style-type: square;
	list-style-position: inside;
}
.column_center ol {
	list-style-type: decimal;
	list-style-position: inside;
  	margin-bottom: 15px;
}



/* 4.3 LINKS */
a {
	color: {{ settings.link_color }};
	text-decoration: none;
}
a:hover, a:focus {
	color: {{ settings.link_color_act }};
}



/* 4.4 PRODUCT ATTRIBUTES (name, description, price) */
.product_name {
	font-weight: bold;
	font-size: {{ settings.product_name_font_size }};
	color: {{ settings.product_name_color }};
  	position: relative;
  	z-index: 1;
}
.product_name a {
	font-weight: bold;
  	text-transform: uppercase;
	color: {{ settings.product_name_color }};
	{{ trans_fast }};
}
.product_name a:hover {
	color: {{ settings.product_name_color_act }};
}


.product_desc {
	font-size: {{ settings.product_desc_font_size }};
	color: {{ settings.product_desc_color }};
}


.money {
  	font-weight: 300;
	color: {{ settings.product_price_color }};
}
.compare-at-price {
	text-decoration: line-through;
}



/* 4.5 RTE */
.rte {
	margin-top: 15px;
	margin-bottom: 15px;
	color: inherit;
}

.rte a {
	color: inherit;
	text-decoration: underline;
}
.rte a:hover {
	text-decoration: none;
}

.rte h1 {
	font-size: 19px;
}
.rte h2,
.column_center h2,
h2 {
	font: 300 {{ settings.heading_font_size }}/40px {{ heading_font }};
	color: {{ settings.heading_font_color }};
	text-transform: uppercase;
  	vertical-align: baseline;
  	letter-spacing: 3px;
}
.rte h3,
.column_center h3,
h3 {
	font: 300 18px/24px {{ heading_font }};
	text-transform: uppercase;
  	vertical-align: baseline;
  	letter-spacing: 3px;
}
.rte h4 {
	font-size: 14px;
}
.rte h5 {
	font-size: 13px;
}
.rte h6 {
	font-size: 12px;
}

.rte img {
	margin-top: 15px;
	margin-bottom: 15px;
}
h1, h2, h3, h4, h5, h6 {
 	font-family: {{ heading_font }}; 
}
.text1 {
 	text-transform: uppercase; 
  	letter-spacing: 2px;
}

.page_block_title {
 	margin-top: 30px;  
}

/***************************************************************************
                           5. FORMS & COMPONENTS
***************************************************************************/

/* 5.1 FORMS */
label {
	font-weight: bold;
	font-size: {{ settings.regular_font_size}};
}

select,
select.form-control,
textarea,
textarea.form-control,
input[type="text"],
input[type="email"],
input[type="number"],
input[type="password"],
input[type="telephone"] {
	padding: 6px;
	border:1px solid {{ settings.border_color }};
	font: normal 13px 'Arial';
	color: {{ settings.font_color }};
	-webkit-box-sizing: border-box;
	   -moz-box-sizing: border-box;
	        box-sizing: border-box;
	border-radius: 0;
	box-shadow: none;
}
input[type="text"],
input[type="email"],
input[type="number"],
input[type="password"],
input[type="telephone"] {
 	height: 34px;
  	line-height: 18px;
  	padding-top: 7px;
  	padding-bottom: 7px;
}

select,
select.form-control {
	padding: 8px 3px;
}

textarea,
textarea.form-control {
	min-width: 100%;
	max-width: 100%;
}

.form-control:focus {
	border-color: #aaa;
	-webkit-box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.075), 0 0 8px #ddd;
	        box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.075), 0 0 8px #ddd;
	}


.form-group {
	vertical-align: top !important;
	margin-top: 15px;
	margin-bottom: 0;
}
.form-group .btn,
.form-group span {
 	vertical-align: baseline; 
}


.form-horizontal {
}
.form-horizontal label {
	padding-top: 0 !important;
	text-align: right;
	line-height: 34px;
}
.form-horizontal select {
	width: 100%;
}

.action_bottom span.note {
	margin-left: 5px;
}
.action_bottom span.note a {
	margin-left: 5px;
}



/* 5.2 ALERTS */
.alert {
	margin: 15px 0;
	-webkit-border-radius: 0;
	   -moz-border-radius: 0;
	        border-radius: 0;
	}

.alert > p,
.alert > ul,
.alert > ol {
	list-style-type: none;
	margin: 0;
}

.alert > p + p,
.alert > ul + ul {
	margin: 10px 0 0 0;
}


/* 5.3 BUTTONS */
.btn,
.btn a,
.btn-info,
.btn-primary {
	font-size: 14px;
	text-transform: uppercase;
	background: none !important;
	border-color: none !important;
	border-radius: 0 !important;
	box-shadow: none !important;
	color: {{ settings.color_2 }};
  	padding: 0px;
  	letter-spacing: 2px;
  	{{trans_slow}};
  	border: none !important;
}
.btn i {
	width: 52px;
	height: 52px;
	text-align: center;
	vertical-align: middle;
	border-radius: 50%;
 	font-size: 40px; 
 	color: {{ settings.color_2 }};
  	{{trans_slow}};
}

.btn i:before {
  	line-height: 50px;
}
.btn a {
	padding: 0 !important;
	border: none !important;
}

.btn:hover,
.btn:hover a,
.btn-info:hover,
.btn-primary:hover {
  	color: {{settings.color_3}};
	background: none !important;
	border-color: none !important;
  	border: none !important;
}

.btn:hover i {
 	color: {{ settings.color_1 }};
 	background: {{ settings.color_2 }};
}
.btn:focus {
  	color: {{settings.color_3}} !important;
}


