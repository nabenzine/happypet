/* 3.1 HOVERS */
.hover__1 {
	overflow: hidden;
	position: relative;
}

.hover__1:before,
.hover__1:after {
	content: '';
	width: 100%;
	height: 50%;
	display: block;
	background: rgba({{ rgb }},.2);
	position: absolute;
	left: 0;
	{{ trans_fast }}
}

.hover__1:before {
	top: -100%;
}
.hover__1:after {
	bottom: -100%;
}

.hover__1:hover:before {
	top: 0;
}
.hover__1:hover:after {
	bottom: 0;
}



.hover__2 {
	overflow: hidden;
	position: relative;
}

.hover__2:before,
