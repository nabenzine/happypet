}



.hover__4 {
	overflow: hidden;
	position: relative;
}

.hover__4:before {
	content: '';
	width: 0;
	height: 100%;
	display: block;
	background: rgba({{ rgb }},.2);
	position: absolute;
	left: 50%;
	top: 0;
	{{ trans_fast }}
}

.hover__4:hover:before {
	width: 100%;
