/* 8.2 NAVIGATION */

#navigation {
	position: relative;
	z-index: 90;
  	text-align: center;
}
#navigation:before {
  	content: " ";
	position: absolute;
	left: 0;
	right: 0;
	top: 0;
	bottom: 0;
	background: {{ settings.color_5 }};
	opacity: .5;
  	z-index: 0;
}
#navigation.isStuck:before {
	opacity: .95;
}
#navigation nav {
	position: relative;
	z-index: 9;
}

#navigation.isStuck {
	width: 100%;
	left: 0;
	top: 0;
}
.sf-menu {
  	display: block;
	position: relative;
  	padding: 0 15px;
}
.sf-menu li {
	position: relative;
}

/*================================>> 1 Level <<========================================*/

.sf-menu > li {
	display: block;
  	float: left;
  	width: 16.6%;
}
.sf-menu > li + li {
  	border-left: 1px solid {{ settings.color_6 }};
}
.sf-menu > li > a {
	display: block;
	position: relative;
	color: {{ settings.color_4 }};
	font-size: 18px;
	line-height: 26px;
	padding: 23px;
  	text-transform: uppercase;
  	letter-spacing: 1px;
}

.sf-menu > li > a .hover {
  	position: absolute;
	top: 0;
	left: 0;
	right: 0;
	border-top: 0px solid {{ settings.color_4 }};
  	height: 0px;
  	overflow: hidden;
  	{{trans_slow}};
}
.sf-menu > li > a .hover:after {
  	position: absolute;
  	top: -21px;
  	left: 50%;
  	margin-left: -13px;
	content: "\f0d7";
  	font-size: 35px;
  	line-height: 30px;
  	display: inline-block;
    font-family: FontAwesome;
    font-style: normal;
    font-weight: normal;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
  	{{trans_slow}};
}
.sf-menu > li > a:hover .hover:after,
.sf-menu > li.active a .hover:after {
  	top: -11px;
}
.sf-menu > li.active > a > .hover,
.sf-menu > li:hover > a > .hover {
	border-top: 5px solid {{ settings.color_4 }};
  	height: 20px;
}

.sf-menu > li > a.sf-with-ul:after {
	position: absolute;
	content:"";
	left: 50%;
	margin-left: -2px;
	right: 0;
	bottom: 12px;
	-moz-transition: top 0.3s ease-out;
    -o-transition: top 0.3s ease-out;
    -webkit-transition: top 0.3s ease-out;
	border: 4px solid;
	border-color: {{ settings.color_4 }} transparent transparent transparent;
	width: 0;
	height: 0;
}

/*================================>> 2 Level <<========================================*/

.sf-menu ul {
	width: 100% !important;
	position: absolute;
	left: 0;
	top: 71px;
	z-index: 100;
	background: {{ settings.color_5 }};
  	text-align: left;
  	display: none;
  	border: 1px solid {{ settings.color_6 }};
  	border-top: 0px;
}
.sf-menu ul li {
	padding: 5px 15px;
	border-top: 1px solid {{ settings.color_6 }};
}
.sf-menu ul li a {
	display: block;
	color: {{ settings.color_4 }};
  	text-transform: uppercase;
}

.sf-menu ul > li a:hover,
.sf-menu ul > li.active a {
	color: {{ settings.color_3 }};
}


/* 8.4 SHOWCASE */

.custom_showcase {
  	margin-top: 55px;
}
.custom_showcase a {
	position: relative;
  	display: block;
}
.custom_showcase .circle {
	position: absolute;
	bottom: 26px;
	width: 100%;
	height: 270px;
	border-radius: 50%;
  	text-align: center;
    transform-style: preserve-3d;
    transition: all 0.6s linear, opacity .4s linear;
}
.custom_showcase a:hover .circle {
  	transform: rotateY(180deg);
  	opacity: 0;
}
.custom_showcase .circle .ins {
  	position: relative;
  	z-index: 1;
    width: 78%;
    margin: 0 auto;
  	padding-top: 72px;
}
.custom_showcase .circle h2,
.custom_showcase .circle h3 {
	color: {{ settings.color_1 }};
}
.custom_showcase .circle h2 {
 	line-height: 36px; 
}
.custom_showcase h2 span {
	color: {{ settings.color_8 }};
}
.showcase_bg {
 	position: absolute;
 	left: 0;
 	right: 0;
 	top: 0;
 	bottom: 0;
 	z-index: 0;
 	opacity: .8;
  	border-radius: 50%;
 }
.custom_showcase__1 .circle .showcase_bg {
	background: {{ settings.color_2 }};
}
.custom_showcase__2 .circle .showcase_bg {
	background: {{ settings.color_7 }};
}
.custom_showcase__3 .circle .showcase_bg {
	background: {{ settings.color_8 }};
}
.custom_showcase__4 .circle .showcase_bg {
	background: {{ settings.color_3 }};
}

.custom_showcase__3 .circle .ins * {
	color: {{ settings.color_2 }};
}
.custom_showcase__4 h2 span {
	color: {{ settings.color_1 }};
}
.custom_showcase a .circle .showcase_bg {
  	{{trans_slow}}; 
}
.custom_showcase a:hover .circle .showcase_bg {
  	background: {{settings.color_1}}; 
}
.custom_showcase .circle .ins:after {
	position: absolute;
	content: "\f107";
	left: 50%;
  	bottom: -30px;
	margin-left: -13px;
	font-size: 38px;
	color: {{ settings.color_1 }};
  	display: inline-block;
    font-family: FontAwesome;
    font-style: normal;
    font-weight: normal;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}
.custom_showcase__3 .circle .ins:after {
	color: {{ settings.color_2 }};
}

